page {
  background-color: #f5f5f5;
}

.box {
  padding: 0 20rpx;
}

.title {
  padding: 20rpx 0;
  margin-top:30rpx;
}

.item {
  border-radius: 20rpx;
  background-color: #fff;
  padding: 20rpx 30rpx;
  box-shadow: 5rpx 5rpx 5px #ccc;
  margin: 20rpx 0;
}

.newbox {
  border-radius: 30rpx;
  margin-bottom: 10rpx;
  background-color: antiquewhite;
  margin-top: 30rpx;
  margin-left: 8rpx;
  margin-right: 10rpx;

}


.fxitem {
  display: flex;
  justify-content: space-between;
  padding: 40rpx;
  align-items: center;
  border-bottom: 1px solid #ecc6c6;
  text-align: right;
}

.center {
  margin-top:30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}