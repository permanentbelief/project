.img {
    width: 100%;
    height: 400rpx;
    position: relative;
    z-index: -1;
}

page {
    background-color: #f5f5f5;
}

.font {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 20rpx;
    color: #fff;
    font-size: 40rpx;
    letter-spacing: 10rpx;
}

.shuru {
    background-color: #fff;
    z-index: 100;
    margin: -100rpx 30rpx 0;
}

.shuru view {
    padding: 30rpx;
}

.shuru view {
    display: flex;
    justify-content: space-between;

}

.shuru view:first-child {
    border-bottom: 1px solid #f7f7f7;
}

.shuru input {
    text-align: right;
}


.btn {
    background-color: #2180eb;
    margin: 40rpx 30rpx;
    padding: 20rpx 0;
    color: #fff;
}

.concat {
  margin: 30rpx;
  font-size: 35rpx;
  color: #2480f0;
  text-decoration: underline;
  text-align: right;
}

.tip {
    padding: 30rpx;
}
.tip view {
    margin-top: 20rpx;
    color: #787878;
}