    margin: 10px;
    display: flex;
    background: #fff;
    height: 20px;
    border-radius: 6px;
    padding: 12px;
}

.home-gg2 {
    display: flex;
    padding-left: 5px;
}

.home-gg2>view:nth-child(1) {
    transform: skewX(-30deg);
    width: 44px;
    height: 20px;
    background: linear-gradient(to top left, #f7ce9e, #d69c63);
    text-align: center;
    color: #fff;
    border-radius: 3px;
}

.home {
    background: #f8f8f8;
    height: 99.5%;
    width: 100%;
    animation: fadeIn .6s;
    min-height: 99.5vh;
}

.home-gg2>view:nth-child(2) {
    font-size: 13px;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-family: PingFang;
    font-weight: 400;
    margin-left: 10px;
    width: 80%;
    overflow: hidden;
    color: rgba(51, 51, 51, 1);
}

.home-gg2>view:nth-child(1) view {
    display: inline-block;
    transform: skewX(30deg);
}

