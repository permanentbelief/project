.temp {
    height: 100rpx;
    background-color: #353fdf;
}

.detail {
    background-color: #fff;
    border-radius: 30rpx;
    margin-top: -50rpx;
    padding: 0 20rpx;
}

.up {
    border-bottom: 1px solid #fbfbfb;
    padding: 30rpx 0 20rpx;
    display: flex;
    flex-direction: column;
}   

.top>text:first-child {
    color: #d10807;
    font-size: 35rpx;
}

.top>text:last-child {
    margin-left: 20rpx;
}

.btom {
    padding-left: 60rpx;
    font-size: 26rpx;
    color: #ccc;
    margin-top: 10rpx;
}

.top text:first-child {
    font-size: 40rpx;
    color: #d10807;
}

.last {
    display: flex;
    margin-top: 20rpx;
    background-color: #fdf7f7;
}

.last>view:first-child {
    background-color: #fce7e7;
    border-radius: 20rpx;
    padding: 20rpx;
    letter-spacing: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.right text:first-child {
    margin: 0 30rpx;
}

.item view:not(:first-child) {
    margin-top: 20rpx;
}


