}

.last>view:first-child text {
    writing-mode: vertical-rl;
}

.item view {
    flex-direction: column;
}

.yichu {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
