/**app.wxss**/
@import '/icon/iconfont.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.btn {
  display: flex;
  justify-content: space-around;
}

.btn view {
  display: flex;
  flex-direction: column;
}

.warp {
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
}