/* pages/feedback/feedback.wxss */

page {
    background-color: #eeeeee;
}

.fd_main {
    padding: 20rpx;
    color: #666;
}

.choose{
    color: red;    
    
}
.fd_main .fd_tips {
    display: flex;
    flex-wrap: wrap;
}
.fd_main .fd_title{
    font-weight: bold;
    font-size: 30rpx;
    color: rgb(78, 99, 99);    
}

.fd_main .fd_tips text {
    width: 70px;
    height: 15%;
    padding: 10rpx;
    text-align: center;
    background-color:whitesmoke;
    margin: 20rpx;
}

.fd_main .fd_content {
    background-color: #fff;
    margin-top: 20rpx;
}

.fd_main .fd_content textarea {
    padding: 10rpx;
}

.fd_main .fd_content .fd_tool {
    display: flex;
    flex-wrap: wrap;
    padding-bottom: 10rpx;
}

.fd_main .fd_content .fd_tool button {
    margin: 0;
    width: 90rpx;
    height: 90rpx;
    margin-top: 20rpx;
    font-size: 40rpx;
    padding: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 20rpx;
    margin-bottom: 10rpx;
    font-size: 60%;
    color: #ccc;
}

.fd_main .fd_content .fd_tool .up_img_item {
    margin-left: 20rpx;
    margin-top: 20rpx;
}

.fd_main .form_btn_wrap {
    margin-top: 20rpx;
    display: flex;
    justify-content: flex-end;
}

.fd_main .form_btn_wrap button {
    background-color: red;
    width: 35%;
    margin: 0;
}

.star{
    text-align: center;
    padding: 30rpx;
    margin: 20rpx;
    background: #fff;
    border-radius: 20rpx;
  }
  .text{
    margin-top: 20rpx;
    font-size: 28rpx;
    color: #666;
  }

  .vant{
      width: 70%;
      border-radius: 40rpx;
  }